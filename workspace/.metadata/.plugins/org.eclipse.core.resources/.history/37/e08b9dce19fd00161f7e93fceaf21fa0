package string;

public class Palindrom {
	public static void main(String[] args) {
		String s="geeksskeeg";
		char[] a=s.toCharArray();
		int m=0,i,l=s.length();
	System.out.println(l);
		for(i=0;i<=(l-1)/2;i++){
			if(a[i]==a[l-1-i])
			{
				m++;
			}else
				break;
				
		}System.out.println(m);
		if(m==l/2){
			System.out.println("palindrom");
		}else{
			System.out.println("not");
		}
	}
	

}
