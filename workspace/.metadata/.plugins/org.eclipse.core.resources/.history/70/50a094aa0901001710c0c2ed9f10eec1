package string;

public class Remove_duplicate_Charaters {
public static void main(String[] args) {
	String s="geeksforgeeks";
	int l=s.length();
	char[] a=s.toCharArray();
	int i,j;
	System.out.println(a[12]+l);
	for(i=0;i<=l;i++){
		if (a[i]==a[i+1]){
			for(j=i+1;j<=l-1;j++){
				a[j]=a[j+1];
				l--;
			}
		}
	}
	
}
}
