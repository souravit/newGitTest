package string;

public class Occuring_charater {
public static void main(String[] args) {
	String s="ababbaec";
	char[] a =s.toCharArray();
	int l=s.length();
	int i,j,c=0;
	for(i=0;i<l-1;i++){
		for(j=0;j<l-1;j++){
			//c=0;
			if (j<i && a[i]==a[j]){
				break;
			}
			if(a[j]==a[j]){
				c++;
				System.out.print(c);
			}
			if(j==l-1){
				System.out.print("the charater "+a[i]+" is present "+c+" times");
		}
			
		}
	}
}
}
