package string;

public class Remove_duplicate_Charaters {
public static void main(String[] args) {
	String s="ram";
	int l=s.length();
	char[] a=s.toCharArray();
	int i,j;
	System.out.println(l);
	for(i=0;i<l;i++){
		if (a[i]==a[i+1]){
			for(j=i+1;j<l;j++){
				a[j]=a[j+1];
				l--;
			}
			System.out.println(l);
		}
	}
	
}
}
